    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

        <com.synnapps.carouselview.CarouselView
                android:id="@+id/imageslider"
                android:layout_height="210dp"
                android:layout_width="match_parent"
                android:layout_marginBottom="3dp"
                app:fillColor="#FFFFFFFF"
                app:pageColor="#00000000"
                app:radius="6dp"
                app:slideInterval="3000"
                app:strokeColor="#FF777777"
                app:strokeWidth="1dp"/>

        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_below="@+id/imageslider"
            tools:context=".BerandaFragment">

            <LinearLayout
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_margin="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1">
                             <ImageView
                                android:id="@+id/berita"
                                android:scaleType="fitXY"
                                android:layout_gravity="center"
                                android:layout_width="60dp"
                                android:layout_height="60dp"
                                android:src="@drawable/berita"/>
                             <TextView
                                android:layout_marginTop="5dp"
                                android:layout_gravity="center"
                                android:textColor="@color/hitam"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="BERITA "/>
                        </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                        <ImageView
                            android:id="@+id/pengumuman"
                            android:scaleType="fitXY"
                            android:layout_gravity="center"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:src="@drawable/pengumuman"
                            android:layout_weight="1"/>
                        <TextView
                            android:layout_marginTop="5dp"
                            android:layout_gravity="center"
                            android:textColor="@color/hitam"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="PENGUMUMAN "/>
                    </LinearLayout>
                </LinearLayout>
        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/potensinagari"
                    android:scaleType="fitXY"
                    android:layout_gravity="center"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:src="@drawable/potensidesa"/>
                <TextView
                    android:layout_marginTop="5dp"
                    android:layout_gravity="center"
                    android:textColor="@color/hitam"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="POTENSI NAGARI"/>
            </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
            <ImageView
                android:id="@+id/programnagari"
                android:scaleType="fitXY"
                android:layout_gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/programnagari"
                android:layout_weight="1"/>
            <TextView
                android:layout_marginTop="5dp"
                android:layout_gravity="center"
                android:textColor="@color/hitam"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="PROGRAM NAGARI"/>
        </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/galeri"
                    android:scaleType="fitXY"
                    android:layout_gravity="center"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:src="@drawable/galeri"/>
                <TextView
                    android:layout_marginTop="5dp"
                    android:layout_gravity="center"
                    android:textColor="@color/hitam"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="GALERI "/>
            </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
            <ImageView
                android:id="@+id/video"
                android:scaleType="fitXY"
                android:layout_gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/video"
                android:layout_weight="1"/>
            <TextView
                android:layout_marginTop="5dp"
                android:layout_gravity="center"
                android:textColor="@color/hitam"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="VIDEO"/>
        </LinearLayout>
        </LinearLayout>
        </LinearLayout>




    </RelativeLayout>
    </RelativeLayout>


</android.support.v4.widget.NestedScrollView>